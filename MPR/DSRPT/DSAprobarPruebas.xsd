                    <CommandText>SELECT        MPR_Solic_Pruebas.IdSolicPrueba, MPR_Solic_Pruebas.HeaderSolicPrueba, MPR_Solic_Pruebas.FechaRegistro, MPR_Solic_Pruebas.username, 
